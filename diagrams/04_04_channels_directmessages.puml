@startuml

title Channels

namespace channel <<rectangle>> {

    json channel.Supergroup {
        "id" : 2200315944,
        "isChannel": true,
        "isForum": false,
        "hasLinkedChat": false,
        "hasDirectMessagesGroup": true,
        "isDirectMessagesGroup": false
    }

    json channel.SupergroupFullInfo {
        "description" : "channel with direct messages chat",
        "linkedChatId" : 0,
        "directMessagesChatId": -2302200315944
    }

    json channel.Chat {
        "id" : -1002200315944,
        "type": {
            "_type" : "ChatTypeSupergroup",
            "supergroupId": 2200315944,
            "isChannel": true
        }
    }
    
    Supergroup ->SupergroupFullInfo : getSupergroupFullInfo
}  


namespace directMessages <<rectangle>> {
